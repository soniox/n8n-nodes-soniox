{
    "name": "@soniox/n8n-nodes-soniox",
    "version": "0.1.0",
    "description": "n8n community node for Soniox speech-to-text transcription",
    "license": "MIT",
    "homepage": "https://soniox.com",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "soniox",
        "speech-to-text",
        "transcription",
        "stt",
        "audio",
        "voice"
    ],
    "author": {
        "name": "Soniox Inc.",
        "url": "https://soniox.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/soniox/n8n-nodes-soniox.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist/**/*",
        "CHANGELOG.md",
        "README.md"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/SonioxApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Soniox/Soniox.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/node": "^25.0.9",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
